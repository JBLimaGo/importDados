var
  Data_Modulo: TData_Modulo;

implementation

uses U_Importa_Dados;

{$R *.dfm}

procedure TData_Modulo.DataModuleCreate(Sender: TObject);
var
  Registro     : TRegistry;

begin

  Data_Modulo.DB.Connected := False;
  Registro     := TRegistry.Create;
  try
   if Registro.OpenKey('\Software\Desenvol\Aplicativo', False) then
      Data_Modulo.DB.DBName:= Registro.ReadString('Server') + ':' + Registro.ReadString('DatabaseName');

      F_Importacao.L_Caminho_Banco.Caption := Data_Modulo.DB.DBName;
  finally
    Registro.CloseKey;
    Registro.Free;
  end;

  Registro := nil;

  try
      with Data_Modulo.DB do
        begin
          Connected := True;
       end;

  except on E:exception do
      begin
         Data_Modulo.DB.Connected := false;
      end;
  end;
end;
end.
